		"main": [
			"ploopy.redstoneschematicbuilder.RedstoneSchematicBuilder"
		],
        "client": [
          "ploopy.redstoneschematicbuilder.RedstoneSchematicBuilderClient"
        ],
		"fabric-datagen": [
			"ploopy.redstoneschematicbuilder.RedstoneSchematicBuilderDataGenerator"
		]
	},
	"mixins": [
		"redstoneschematicbuilder.mixins.json"
	],
	"depends": {
